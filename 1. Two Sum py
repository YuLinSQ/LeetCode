class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        hashmap = {}
        for i in range (len(nums)):
            if nums[i] not in hashmap:
                hashmap[nums[i]] = i
        print(hashmap)
        for i in range (len(nums)):
            temp = target - nums[i]
            if temp in hashmap and hashmap[temp] != i:
                return [i, hashmap[temp]]
