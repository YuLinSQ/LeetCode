class Solution {
public:
    string longestCommonPrefix(vector<string>& strs) {
        string ans="";
        string temp;
        int k;
        for(int i=0; i<strs.size(); i++)
        {
            for(int j=i+1; j<strs.size(); j++)
            {
                temp="";
                k=0;
                while(k<min((strs[i].size()),(strs[j].size())))
                {
                    cout<<temp<<"? ";
                    if(strs[i][k]==strs[j][k])
                    {
                        temp+=(strs[i][k]);
                        k++;
                    }
                    else
                    {
                        cout<<i<<"-"<<j<<"-"<<k<<" ";
                        k=strs[k].size();
                    }
                }
                if(temp.length()>ans.length())
                {
                    ans=temp;
                    cout<<ans<<"! ";
                }
            }
        }
        return ans;
    }
};
