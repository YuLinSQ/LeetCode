class Solution(object):
    def sumOfUnique(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        hm = {}
        for i in nums:
            if i not in hm:
                hm[i] = 0
            hm[i]+=1
        ans = 0
        for i in hm:
            if hm[i] == 1:
                ans+=i
        return ans
