class Solution(object):
    def largestNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: str
        """
        ans = ''
        for i in range (len(nums)):
            mind = 0
            mval = nums[0]
            for j in range (1, len(nums)):
                if str(mval)+str(nums[j]) < str(nums[j])+str(mval):
                    mind = j
                    mval = nums[j]
            ans+=str(nums.pop(mind))
        return str(int(ans))

            
