class Solution(object):
    def areAlmostEqual(self, s1, s2):
        """
        :type s1: str
        :type s2: str
        :rtype: bool
        """
        if s1 == s2:
            return True
        temp1 = None
        temp2 = None
        for i in range (len(s1)):
            if s1[i]!=s2[i]:
                if temp1 == None:
                    temp1 = s1[i]
                    temp2 = s2[i]
                elif temp1 == s2[i] and temp2 == s1[i]:
                    if s1[i+1:] == s2[i+1:]:
                        return True
                    return False
                else:
                    return False
        return False
