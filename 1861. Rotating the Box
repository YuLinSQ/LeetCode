class Solution(object):
    def rotateTheBox(self, boxGrid):
        """
        :type boxGrid: List[List[str]]
        :rtype: List[List[str]]
        """
        m = len(boxGrid)
        n = len(boxGrid[0])
        ans = []
        for i in range(n):                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
            ans.append(["."]*m)
        for i in reversed(range(m)):
            ind = n-1
            for j in reversed(range(n)):
                if boxGrid[i][j] == "#":
                    ans[ind][m-i-1] = "#"
                    ind-=1
                elif boxGrid[i][j] == "*":
                    ans[j][m-i-1] = "*"
                    ind = j-1
        return ans
