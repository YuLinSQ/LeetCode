class Solution(object):
    def countSubIslands(self, grid1, grid2):
        """
        :type grid1: List[List[int]]
        :type grid2: List[List[int]]
        :rtype: int
        """
        visit = {}

        def dfs(i,j):
            if (i,j) in visit or i<0 or j<0 or i==len(grid2) or j==len(grid2[0]):
                return True
            if grid2[i][j]==1 and grid1[i][j]==0:
                return False
            visit[(i,j)] = 0
            if grid2[i][j]==0:
                return True
            c1 = dfs(i+1,j)
            c2 = dfs(i-1,j)
            c3 = dfs(i,j+1)
            c4 = dfs(i,j-1)
            return c1 and c2 and c3 and c4

        count = 0
        for i in range(len(grid2)):
            for j in range(len(grid2[0])):
                if (i,j) in visit or grid1[i][j]==0 or grid2[i][j]==0:
                    continue
                check = dfs(i,j)
                if check:
                    count+=1
        return count
                
            
