class Solution(object):
    def rob(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if len(nums) == 0:
            return 0
        if len(nums) == 1:
            return nums[0]
        arr = [nums[0]]
        arr.append(max(nums[0], nums[1]))
        for i in range (2, len(nums)):
            arr.append(max(arr[i-1], arr[i-2]+nums[i]))
        return arr[-1]
