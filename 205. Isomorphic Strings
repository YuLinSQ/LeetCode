class Solution(object):
    def isIsomorphic(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        
        hm1 = {}
        hm2 = {}
        for i in range (len(s)):
            if s[i] not in hm1:
                hm1[s[i]] = t[i]
            elif hm1[s[i]] != t[i]:
                return False
            if t[i] not in hm2:
                hm2[t[i]] = s[i]
            elif hm2[t[i]] != s[i]:
                return False
        return True
