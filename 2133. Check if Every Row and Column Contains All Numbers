class Solution(object):
    def checkValid(self, matrix):
        """
        :type matrix: List[List[int]]
        :rtype: bool
        """
        n = len(matrix)
        for i in range (n):
            s = set()
            for j in range (n):
                s.add(matrix[i][j])
            if len(s) != n:
                return False
        for j in range (n):
            s = set()
            for i in range (n):
                s.add(matrix[i][j])
            if len(s) != n:
                return False
        return True
