class Solution(object):
    def minimumOperations(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        
        hm = {}
        for i in nums:
            if i in hm:
                hm[i]+=1
            else:
                hm[i] = 1
        if 0 in hm:
            return len(hm)-1
        return len(hm)
