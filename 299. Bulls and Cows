class Solution(object):
    def getHint(self, secret, guess):
        """
        :type secret: str
        :type guess: str
        :rtype: str
        """
        hm = {}
        a, b = 0,0
        for i in secret:
            if i not in hm:
                hm[i] = 0
            hm[i]+=1
        for i in range(len(guess)):
            if guess[i] in hm:
                if secret[i] == guess[i]:
                    a+=1
                    if hm[guess[i]]==0:
                        b-=1
                    else:
                        hm[guess[i]]-=1
                else:
                    if hm[guess[i]]>0:
                        hm[guess[i]]-=1
                        b+=1
        return str(a)+"A"+str(b)+"B"
