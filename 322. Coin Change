class Solution(object):
    def coinChange(self, coins, amount):
        """
        :type coins: List[int]
        :type amount: int
        :rtype: int
        """
        arr = [amount+1]*(amount+1)
        arr[0] = 0
        for i in range(1, amount+1):
            for c in coins:
                if i - c >= 0:
                    arr[i] = min(arr[i], arr[i-c]+1)
        if arr[-1] != amount+1:
            return arr[-1]
        return -1
            
