class Solution(object):
    def search(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        i, j = 0,len(nums)-1
        while i<=j:
            temp = (i+j)//2
            if nums[temp] == target:
                return temp
            if nums[temp] >= nums[i]:
                if target > nums[temp] or nums[i] > target:
                    i = temp+1
                else:
                    j = temp-1 
            else:
                if target > nums[j] or target < nums[temp]:
                    j = temp-1
                else:
                    i = temp+1
        return -1
            
                
