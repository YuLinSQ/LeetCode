class Solution(object):
    def firstUniqChar(self, s):
        """
        :type s: str
        :rtype: int
        """
        hm = {}
        for i in range (len(s)):
            if s[i] not in hm:
                hm[s[i]] = i
            else:
                hm[s[i]] = -1
        for i in s:
            if hm[i] >= 0:
                return hm[i]
        return -1
