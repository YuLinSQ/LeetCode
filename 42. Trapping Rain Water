class Solution(object):
    def trap(self, height):
        """
        :type height: List[int]
        :rtype: int
        """
        i = 0
        j = len(height)-1
        ans = 0
        lm = 0
        rm = 0
        while i!=j:
            if height[i] <= height[j]:
                ans+=max(0,lm-height[i])
                lm = max(lm,height[i])
                i+=1
            else:
                ans+=max(0,rm-height[j])
                rm = max(rm,height[j])
                j-=1
        return ans
