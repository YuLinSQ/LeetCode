class Solution(object):
    def findAnagrams(self, s, p):
        """
        :type s: str
        :type p: str
        :rtype: List[int]
        """
        ans = []
        hm = {}
        for i in p:
            if i not in hm:
                hm[i] = 0
            hm[i]+=1
        copy = {}
        i = 0
        for j in range(len(s)):
            if s[j] in hm:
                if s[j] not in copy:
                    copy[s[j]] = 0
                copy[s[j]]+=1
                if len(p) == j-i+1:
                    if copy == hm:
                        ans.append(i)
                    copy[s[i]]-=1
                    i+=1
            else:
                copy = {}
                i = j+1
        return ans
