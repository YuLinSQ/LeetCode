class Solution(object):
    def permute(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        ans = []
        nums.sort()

        def dfs(arr, cur):
            if arr == []:
                ans.append(cur[:])
            for i in range (len(arr)):
                cur.append(arr[i])
                dfs(arr[:i] + arr[i+1:], cur)
                cur.pop()

        dfs(nums, [])
        return ans
