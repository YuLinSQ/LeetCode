class Solution(object):
    def nextGreaterElement(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: List[int]
        """
        hm = {}
        for i in range(len(nums2)):
            hm[nums2[i]] = i
        for i in range(len(nums1)):
            j = hm[nums1[i]]+1
            while j<len(nums2):
                if nums2[j]>nums1[i]:
                    nums1[i] = nums2[j]
                    j = len(nums2)+1
                j+=1
            if j == len(nums2):
                nums1[i] = -1
        return nums1
# could use stack to reduce time i think [1,3,4], [3,1,4,5] -> [3,1,4],
# but that would be m time, and if n is small, it's less efficient

        
