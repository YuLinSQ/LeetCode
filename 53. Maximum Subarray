class Solution(object):
    def maxSubArray(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if nums == []:
            return 0
        ans = nums[0]
        temp = 0
        for i in range(len(nums)):
            temp+=nums[i]
            if temp>ans:
                ans = temp
            if temp<0:
                temp = 0
        return ans
