class Solution(object):
    def partitionLabels(self, s):
        """
        :type s: str
        :rtype: List[int]
        """
        hm = {}
        for i in s:
            if i not in hm:
                hm[i] = 0
            hm[i]+=1
        ans = []
        temp = {}
        count = 0
        for i in s:
            if i not in temp:
                temp[i] = 0
            temp[i]+=1
            count+=1
            if temp[i] == hm[i]:
                temp.pop(i)
                if len(temp) == 0:
                    ans.append(count)
                    count = 0
        return ans
