#  referenced quagmire
class Solution(object):
    def subsets(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        ans = []
        sub = []

        def dfs(i):
            if i>=len(nums):
                ans.append(list(sub))
                return
            sub.append(nums[i])
            dfs(i+1)
            sub.pop()
            dfs(i+1)
        
        dfs(0)
        return ans
