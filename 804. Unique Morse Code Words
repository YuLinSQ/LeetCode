class Solution(object):
    def uniqueMorseRepresentations(self, words):
        """
        :type words: List[str]
        :rtype: int
        """
        arr = [".-","-...","-.-.","-..",".","..-.","--.","....","..",".---","-.-",".-..","--","-.","---",".--.","--.-",".-.","...","-","..-","...-",".--","-..-","-.--","--.."]
        hm = {}
        for i in words:
            temp = ""
            for j in i:
                temp+=arr[ord(j)-97]
            if temp not in hm:
                hm[temp] = temp
        return len(hm)
