class Solution(object):
    def fairCandySwap(self, aliceSizes, bobSizes):
        """
        :type aliceSizes: List[int]
        :type bobSizes: List[int]
        :rtype: List[int]
        """
        diff = 0
        hm1 = {}
        for i in aliceSizes:
            diff+=i
            if i not in hm1:
                hm1[i] = 0
            hm1[i]+=1
        hm2 = {}
        for i in bobSizes:
            diff-=i
            if i not in hm2:
                hm2[i] = 0
            hm2[i]+=1
        diff = diff//2
        if diff>=0:
            for i in aliceSizes:
                temp = i-diff
                if temp>=0:
                    if temp in hm2:
                        return [i,temp]
        for i in aliceSizes:
            temp = i-diff
            if temp>=0:
                if temp in hm2:
                    return [i,temp]
                
        
